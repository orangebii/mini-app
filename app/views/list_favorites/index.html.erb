<p id="notice"><%= notice %></p>

<h1>List Favorites</h1>

<table>
  <thead>
    <tr>
      <th>Id list</th>
      <th>Id user</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @list_favorites.each do |list_favorite| %>
      <tr>
        <td><%= list_favorite.id_list %></td>
        <td><%= list_favorite.id_user %></td>
        <td><%= link_to 'Show', list_favorite %></td>
        <td><%= link_to 'Edit', edit_list_favorite_path(list_favorite) %></td>
        <td><%= link_to 'Destroy', list_favorite, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New List Favorite', new_list_favorite_path %>
